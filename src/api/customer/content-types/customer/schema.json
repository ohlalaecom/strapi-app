{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customers",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "FullName": {
      "type": "string",
      "required": false,
      "maxLength": 300
    },
    "Email": {
      "type": "string",
      "required": true,
      "maxLength": 300
    },
    "Contact_1": {
      "type": "string",
      "required": false,
      "maxLength": 15
    },
    "Contact_2": {
      "type": "string",
      "required": false,
      "maxLength": 15
    },
    "Gender": {
      "type": "string",
      "required": false,
      "maxLength": 10,
      "default": "NA"
    },
    "DOB": {
      "type": "date",
      "default": "1900-01-01"
    },
    "Address": {
      "type": "text"
    },
    "City": {
      "type": "string",
      "required": false,
      "maxLength": 300
    },
    "Area": {
      "type": "string",
      "maxLength": 300
    },
    "Country": {
      "type": "string",
      "maxLength": 300,
      "required": false
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    },
    "cust_addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cust-address.cust-address",
      "mappedBy": "customer_id"
    },
    "password": {
      "type": "password"
    },
    "prrovider": {
      "type": "string"
    },
    "resetPasswordToken": {
      "type": "string"
    },
    "confirmationToken": {
      "type": "string"
    },
    "confirmed": {
      "type": "boolean"
    },
    "blocked": {
      "type": "boolean"
    },
    "role": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
